[run]
source = .
omit =
    tests/*
    */__pycache__/*
    */site-packages/*
    .venv/*
    venv/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if __name__ == .__main__.:
    raise NotImplementedError
    pass
    raise ImportError
    except ImportError:
    def main\(\):

[html]
directory = coverage_html